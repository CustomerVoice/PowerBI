Branch Service NPS = 
VAR Responses = COUNTROWS('Customer Voice Survey Response')
VAR Promoters = COUNTROWS(FILTER('Customer Voice Survey Response', [b4bfc221-16da-4c52-8c37-37a489950d21]="promoter"))
VAR Detractors = COUNTROWS(FILTER('Customer Voice Survey Response',[b4bfc221-16da-4c52-8c37-37a489950d21]="detractor"))
VAR PromotorsPercent = DIVIDE(Promoters, Responses)
VAR DetractorsPercent = DIVIDE(Detractors, Responses)
RETURN 
    (PromotorsPercent - DetractorsPercent) * 100

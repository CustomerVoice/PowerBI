Homeloan NPS = 
VAR Responses = COUNTROWS('Customer Voice Survey Response')
VAR Promoters = COUNTROWS(FILTER('Customer Voice Survey Response', [Homeloan Journey NPS]="promoter"))
VAR Detractors = COUNTROWS(FILTER('Customer Voice Survey Response',[Homeloan Journey NPS]="detractor"))
VAR PromotorsPercent = DIVIDE(Promoters, Responses)
VAR DetractorsPercent = DIVIDE(Detractors, Responses)
RETURN 
    (PromotorsPercent - DetractorsPercent) * 100

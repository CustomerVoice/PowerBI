Overall NPS = 
VAR Responses = COUNTROWS('Customer Voice Satisfaction Metric')
VAR Promoters = COUNTROWS(FILTER('Customer Voice Satisfaction Metric', [42d7cb6a-6863-42f2-9431-0c6004ceee00]="promoter"))
VAR Detractors = COUNTROWS(FILTER('Customer Voice Satisfaction Metric',[42d7cb6a-6863-42f2-9431-0c6004ceee00]="detractor"))
VAR PromotorsPercent = DIVIDE(Promoters, Responses)
VAR DetractorsPercent = DIVIDE(Detractors, Responses)
RETURN 
    (PromotorsPercent - DetractorsPercent) * 100
